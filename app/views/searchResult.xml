<Alloy>
	<Window backgroundColor="red" title="Result" id='searchResultWin'>
	<View class="container">
		
		<TableView id='listTable' dataCollection="people" dataFilter="dofilter" dataTransform='transformfunction' editable="true">
			<TableViewRow no='{no}' height="55">
				<ImageView image="default_default_default.png" width="50" height="50" left="10" />
				<Label id="listList" text="{title}" class="listList"></Label>
				</TableViewRow>				
			</TableView>
	</View>
	</Window>
</Alloy>