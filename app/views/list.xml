<Alloy>
	<Window title='Mate List' id='listWin' barColor="#9BEAE8" translucent="false">
	<View class="container">
		
		<TableView id='listTable' dataCollection="people" dataTransform='transformfunction' editable='true'>
			<SearchBar barColor="#FFF" hintText="Search" id="listSearch" showCancel="false" height="43" top="0" ></SearchBar>
			<TableViewRow no='{no}' height='55'>
				<ImageView image="defaultList.png" id='defaultImage' width='50' height='50' left='10'>
				<ImageView image="{PicName}" width='50' height='50' left="10" />
				<Label id='listList' text='{title}' class='listList'></Label>
			</TableViewRow>
				
			</TableView>
	</View>
	</Window>
</Alloy>